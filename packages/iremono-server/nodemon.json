{
  "restartable": "rs",
  "ignore": [".git", "node_modules/**/node_modules"],
  "verbose": false,
  "events": {
    "restart": ""
  },
  "watch": ["dist/index.js"],
  "env": {
    "NODE_ENV": "development"
  },
  "execMap": {
    "js": "node --experimental-specifier-resolution=node"
  },
  "ext": "js,json",
  "delay": 1
}
